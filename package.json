{
  "name": "deep.clone",
  "version": "0.3.0",
  "description": "Simple package to deeply clone any objects with nested objects",
  "keywords": [
    "array",
    "clone",
    "clone-deep",
    "clone.deep",
    "deep",
    "deep-clone",
    "deep.clone",
    "motss",
    "number",
    "object",
    "string"
  ],
  "homepage": "https://github.com/motss/deep.clone",
  "license": "MIT",
  "author": "motss <wes.ngrongsen@gmail.com>",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/deep.clone.git"
  },
  "scripts": {
    "build": "sh ./run-build.sh production",
    "build:clean": "sh ./run-build.sh clean production",
    "build:test": "sh ./run-build",
    "check": "npm run nsp && npm run snyk",
    "codecov": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./src",
    "nsp": "nsp check",
    "nyc": "nyc report --reporter=lcov",
    "prepublish": "npm run lint && npm run check && npm run build",
    "snyk": "snyk test",
    "test": "nyc ava && npm run nyc"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.14.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-babili": "0.1.2",
    "babel-preset-env": "^1.1.8",
    "codecov": "^2.1.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.8.0",
    "eslint-config-google": "^0.8.0",
    "eslint-plugin-optimize-regex": "^1.1.1",
    "eslint-plugin-security": "^1.3.0",
    "nsp": "^2.6.2",
    "nyc": "^11.0.2",
    "snyk": "^1.41.0"
  },
  "engines": {
    "node": ">= 6.9.1",
    "npm": ">= 3.10.8"
  },
  "nyc": {
    "exclude": [
      "{,**}/!(index.js)",
      "test",
      "test{,-*}.js",
      "**/*.test.js",
      "**/__tests__/**",
      "**/node_modules/**"
    ]
  },
  "snyk": true
}
