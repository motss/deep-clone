{
  "name": "deep.clone",
  "version": "1.0.0",
  "description": "Simple package to deeply clone any objects with nested objects",
  "keywords": [
    "array",
    "clone",
    "clone-deep",
    "clone.deep",
    "deep",
    "deep-clone",
    "deep.clone",
    "motss",
    "number",
    "object",
    "string"
  ],
  "homepage": "https://github.com/motss/deep.clone",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/deep.clone.git"
  },
  "license": "MIT",
  "author": "Rong Sen Ng <wes.ngrongsen@gmail.com> (https://github.com/motss)",
  "files": [
    "dist"
  ],
  "main": "dist/index",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && npm run rollup",
    "jest": "jest --config ./.jestrc.json",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish 'src/**/*.ts*'",
    "rollup": "rollup -c ./rollup.config.js",
    "test": "npm run lint && npm run ts && npm run jest",
    "ts": "tsc -p ./tsconfig.test.json"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "@messageflow/tslint-config": "^1.3.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.18",
    "jest": "^23.6.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-terser": "^4.0.2",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
