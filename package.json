{
  "name": "deep.clone",
  "version": "2.0.0",
  "description": "Simple package to deeply clone any objects with nested objects",
  "keywords": [
    "clone",
    "clone deep",
    "clone-deep",
    "clone.deep",
    "deep",
    "deep clone",
    "deep-clone",
    "deep.clone",
    "motss"
  ],
  "homepage": "https://github.com/motss/deep.clone",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/deep.clone.git"
  },
  "license": "MIT",
  "author": "Rong Sen Ng <wes.ngrongsen@gmail.com> (https://github.com/motss)",
  "files": [
    "dist/*.*js.map",
    "dist/*.*js",
    "dist/*.d.ts"
  ],
  "sideEffects": false,
  "main": "dist/index",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run rollup",
    "clean": "rm -rf dist/",
    "jest": "jest --config ./.jestrc.json",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish",
    "prepublishOnly": "npm run lint && npm run build",
    "rollup": "rollup -c ./rollup.config.js",
    "rollup:test": "cross-env ROLLUP_TEST=true npm run rollup",
    "test": "npm run lint && npm run clean && tsc -p tsconfig.test.json && npm run rollup:test && npm run jest",
    "ts": "npm run clean && tsc",
    "watch": "npm run ts -- --watch"
  },
  "dependencies": {
    "lodash-es": "^4.17.11"
  },
  "devDependencies": {
    "@messageflow/tslint-config": "^1.3.0",
    "@types/jest": "^24.0.11",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^11.13.4",
    "cross-env": "^5.2.0",
    "jest": "^24.7.1",
    "rollup": "^1.10.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
